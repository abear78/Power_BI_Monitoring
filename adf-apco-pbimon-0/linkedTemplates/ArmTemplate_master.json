{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-apco-pbimon-0"
		},
		"ls_azure_sql_db_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-pbimon-db-connection-string"
		},
		"ls_blob_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-pbimon-storage-connection-string"
		},
		"ls_function_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://func-apco-pbimon-0.azurewebsites.net"
		},
		"ls_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-apco-pbimon-0.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-apco-pbimon-0_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_properties_typeProperties_baseUrl')]"
					},
					"ls_azure_sql_db_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_azure_sql_db_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_blob_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_blob_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_function_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('ls_function_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}